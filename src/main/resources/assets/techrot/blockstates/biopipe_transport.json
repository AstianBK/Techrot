{
  "multipart": [

    {   "when": { "east": "true"},
      "apply": { "model": "techrot:extractbioeast", "uvlock": false }
    },
    {   "when": { "west": "true" },
      "apply": { "model": "techrot:extractbiowest", "uvlock": false }
    },
    {   "when": { "north": "true"},
      "apply": { "model": "techrot:extractbionorth", "uvlock": false }
    },
    {   "when": {"south": "true"},
      "apply": { "model": "techrot:extractbiosouth", "uvlock": false }
    },
    {   "when": { "up": "true"},
      "apply": { "model": "techrot:extractbiopipeup", "uvlock": false }
    },
    {   "when": {"down" :  "true"},
      "apply": { "model": "techrot:extractbiopipedown", "uvlock": false }
    },
    {
      "apply": { "model": "techrot:extractbiomiddle", "uvlock": false }
    }
  ]
}